<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Button?>
<?import pack_morpion.ModelController?>


<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            prefWidth="500" prefHeight="500"
            fx:controller="pack_morpion.ModelController"
            fx:id="borderPane"
            stylesheets="@application.css">

    <center>
        <ProgressBar fx:id="progressBar" prefWidth="400" prefHeight="50" progress="0" styleClass="ProgressBar"/>
    </center>

    <top>
        <TextField fx:id="textField" editable="false" prefWidth="400" prefHeight="50" styleClass="textFieldLoad"/>
    </top>
	
</BorderPane>